          <stringProp name="BeanShellSampler.query">String jobIds = vars.get(&quot;jobIds&quot;);

String[] idArray = jobIds.split(&quot;,&quot;);

File csvFile = new File(&quot;/Users/michaelclarke/Desktop/Test_Plans/academy_sprint/jobIds.csv&quot;);
FileWriter csvWriter = new FileWriter(csvFile);

for (String id : idArray) {
    csvWriter.append(id.trim());
    csvWriter.append(&quot;\n&quot;);
}

log.info(&quot;CSV File Path: &quot; + csvFile.getAbsolutePath());

